movable-view {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 3.7037%;
  max-height: 20px;
  width: 32px;
}

movable-area {
  position: fixed;
  top: 0%;
  height: 100%;
  width: 32px;
  z-index: 1005;
}

page {
  --sidebar-item-color: rgba(0, 0, 0, 0.3);
}

@media (prefers-color-scheme: light) {
  Page {
    --sidebar-item-color: rgba(0, 0, 0, 0.3);
  }
}

@media (prefers-color-scheme: dark) {
  Page {
    --sidebar-item-color: rgba(255, 255, 255, 0.3);
  }
}

.sidebar-tips-mask {
  position: fixed;
  top: 0%;
  height: 100%;
  width: 32px;
  z-index: 990;
}

.sidebar-item-mask {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 3.7037%;
  max-height: 20px;
  width: 32px;
}

.sidebar-tips {
  position: fixed;
  background-image: url(data:image/png;base64,);
  background-size: contain;
  z-index: 990;
}

.tips-show-ani {
  transition: top 0.1s linear, right 0.1s ease 0.05s, width 0.1s ease 0.05s, height 0.1s ease 0.05s, margin 0.1s ease 0.05s, opacity 0.1s ease 0.05s;
}

.tips-show {
  right: 50px;
  height: 60px;
  width: 60px;
  opacity: 1;
  color: #000;
}

.tips-hide-ani {
  transition: right 0.15s ease, width 0.15s ease, height 0.15s ease, margin 0.15s ease, opacity 0.15s ease;
}

.tips-hide {
  right: 20px;
  height: 10px;
  width: 10px;
  opacity: 0;
  color: #000;
}

.tips-text-show {
  margin-right: 9px;
  font-size: 25px;
  text-align: center;
  line-height: 60px;
  opacity: 1;
}

.tips-text-ani {
  transition: font-size 0.15s ease, line-height 0.15s ease, margin 0.15s ease, opacity 0.15s ease;
}

.tips-text-hide {
  margin-right: 1.5px;
  font-size: 5px;
  text-align: center;
  line-height: 10px;
  opacity: 0;
}

.sidebar-item {
  display: block;
  text-align: center;
  width: 32px;
  color: var(--sidebar-item-color);
  font-size: 10px;
}

.sidebar-on-hode {
  transition: transform 0.13s ease;
  transform: translateX(-50%);
}

.sidebar-off-hode {
  transition: transform 0.05s ease;
}

.sidebar-on-hode-padding {
  transition: transform 0.11s ease;
  transform: translateX(-18%);
}

.sidebar-on-hode-margin {
  transition: transform 0.07s ease 0.02s;
  transform: translateX(-5%);
}

.sidebar-item-selected {
  color: #10AEFF;
}